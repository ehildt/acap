{
  "name": "@cultify/config-manager",
  "version": "0.5.14",
  "description": "A simple and convenient way to config your apps ;)",
  "author": "Eugen Hildt",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --path ./tsconfig.build.json",
    "build:prod": "nest build --path ./tsconfig.build.prod.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -e ./env/defaults.env -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint-staged": "npx lint-staged --allow-empty",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e ./env/defaults.env -- jest --config ./test/jest-e2e.json",
    "test:clearCache": "jest --clearCache",
    "check-licenses": "licensee --errors-only",
    "prepare": "husky install",
    "ncu:update": "npx npm-check-updates --doctor -u ",
    "ncu:patch": "npx npm-check-updates -t patch --packageFile=./package.json",
    "ncu:validate": "npx npm-check-updates -e 2 --packageFile=./package.json",
    "dependency-cruiser": "npx depcruise -c .dependency-cruiser.js --ts-config tsconfig.json src",
    "dependency-cruiser:graph": "npx depcruise -c .dependency-cruiser.js -x \".+.spec.ts\" -T dot ./src | dot -T png > .dependency-graph.png",
    "ts-unused-exports": "npx ts-unused-exports ./tsconfig.json",
    "gitleaks": "docker run --rm -v $(cd .. && pwd):/app zricethezav/gitleaks -c /app/config-manager/gitleaks.toml detect -v --source=\"/app/config-manager/\" --no-git",
    "docker:login": "docker login docker.hub.ideea.app.corpintra.net",
    "docker:build": "docker build --rm --target prod -t localhost:5000/config-manager .",
    "compose:up": "docker compose up -d mongo redis && docker compose up config-manager",
    "gittyleaks": "~/.local/bin/gittyleaks --find-anything -b -e $ $(cat .gittyleaks)",
    "clinic:doctor": "dotenv -e ./env/clinic.env -- npx clinic doctor -- node ./dist/main.js",
    "clinic:flame": "dotenv -e ./env/clinic.env -- npx clinic flame -- node ./dist/main.js",
    "clinic:bubble": "dotenv -e ./env/clinic.env -- npx clinic bubbleprof -- node ./dist/main.js",
    "clinic:clean": "npx clinic clean",
    "docker:build:prod": "docker build --rm --target prod -t localhost:5000/config-manager .",
    "docker:build:dev": "docker build --rm --target dev -t localhost:5000/config-manager .",
    "docker:tag": "docker image tag localhost:5000/config-manager cultify/config-manager",
    "docker:publish:local": "docker push localhost:5000/config-manager",
    "docker:publish:remote": "docker push cultify/config-manager"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/mongoose": "^9.1.1",
    "@nestjs/passport": "^8.2.2",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "argon2": "^0.28.5",
    "axios": "^0.27.2",
    "cache-manager": "^4.0.1",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "helmet": "^5.1.0",
    "mongoose": "^6.4.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "remove": "^0.1.5",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "short-uuid": "^4.2.0",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/cache-manager": "^4.0.1",
    "@types/cache-manager-ioredis": "^2.0.2",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^18.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.3",
    "@typescript-eslint/parser": "^5.30.3",
    "clinic": "^11.1.0",
    "dependency-cruiser": "^11.11.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sonarjs": "^0.13.0",
    "husky": "^8.0.1",
    "jest": "^28.1.2",
    "jest-extended": "^2.0.0",
    "licensee": "^8.2.0",
    "lint-staged": "^13.0.3",
    "npm-check-updates": "^15.0.1",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "ts-unused-exports": "^8.0.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
