{
  "name": "@litlbyte/config-manager",
  "version": "0.6.9",
  "description": "A simple and convenient way to config your apps ;)",
  "repository": "git@github.com:ehildt/config-manager-server.git",
  "author": {
    "name": "Eugen Hildt",
    "email": "eugen.hildt@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "email": "eugen.hildt@gmail.com"
  },
  "scripts": {
    "build:prod": "nest build --path ./tsconfig.build.prod.json",
    "build": "nest build",
    "licensee": "licensee --errors-only",
    "format": "prettier --write .",
    "gitleaks": "docker run --rm -v $(cd .. && pwd):/app zricethezav/gitleaks -c /app/${PWD##*/}/gitleaks.toml detect -v --source=\"/app/${PWD##*/}/\" --no-git",
    "heroku-postbuild": "npm run build && npm run seed",
    "lint-staged": "npx lint-staged --allow-empty",
    "lint": "eslint .",
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "seed": "ts-node -r tsconfig-paths/register src/seed.ts",
    "start:debug": "nest start --debug --watch  --preserveWatchOutput",
    "start:dev": "dotenv -e ./env/defaults.env -- nest start --watch  --preserveWatchOutput",
    "start:prod": "node dist/main",
    "start:heroku": "node dist/main.js",
    "start": "nest start",
    "test:clearCache": "jest --clearCache",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e ./env/defaults.env -- jest --detectOpenHandles --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "test": "jest --passWithNoTests",
    "ts-unused-exports": "npx ts-unused-exports ./tsconfig.json",
    "ncu:update": "npx npm-check-updates --doctor -u",
    "ncu:validate": "npx npm-check-updates -e 2 --packageFile=./package.json",
    "depcruiser": "npx depcruise -c .depcruiser.cjs --ts-config tsconfig.json src",
    "depcruiser:graph": "npx depcruise -c .depcruiser.cjs -x \".+.spec.ts\" -T dot ./src | dot -T png > .depcruiser-graph.png",
    "depcheck": "npx depcheck ."
  },
  "dependencies": {
    "@fastify/multipart": "^7.4.1",
    "@fastify/static": "^6.9.0",
    "@nestjs/common": "9.3.9",
    "@nestjs/config": "2.3.1",
    "@nestjs/core": "9.3.9",
    "@nestjs/microservices": "9.3.9",
    "@nestjs/mongoose": "9.2.1",
    "@nestjs/platform-fastify": "^9.3.9",
    "@nestjs/swagger": "^6.2.1",
    "cache-manager": "5.1.6",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "joi": "^17.7.1",
    "js-yaml": "^4.1.0",
    "mongoose": "6.9.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "4.1.2",
    "rxjs": "7.8.0",
    "short-unique-id": "4.4.4"
  },
  "devDependencies": {
    "@jest/types": "29.4.3",
    "@nestjs/cli": "9.2.0",
    "@nestjs/schematics": "9.0.4",
    "@nestjs/testing": "9.3.9",
    "@types/cache-manager": "4.0.2",
    "@types/cache-manager-ioredis": "2.0.3",
    "@types/eslint": "8.21.1",
    "@types/jest": "29.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "18.14.0",
    "@types/prettier": "2.7.2",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "depcheck": "^1.4.3",
    "dependency-cruiser": "12.9.0",
    "dotenv-cli": "7.0.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-no-loops": "0.3.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sonarjs": "0.18.0",
    "husky": "8.0.3",
    "jest": "29.4.3",
    "jest-extended": "3.2.4",
    "jest-junit": "15.0.0",
    "licensee": "10.0.0",
    "lint-staged": "13.1.2",
    "npm-check-updates": "16.7.5",
    "prettier": "2.8.4",
    "source-map-support": "0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "ts-unused-exports": "9.0.4",
    "tsconfig-paths": "4.1.2",
    "typescript": "4.9.5"
  }
}
